
[general]
status_path = ~/.local/share/vdirsyncer/status"

[pair calendar]
a = "calendar_local"
b = "calendar_remote"
collections = ["from a", "from b"]
metadata = ["color"]

[storage calendar_local]
type = "filesystem"
path = "~/.config/calendars"
fileext = ".ics"

[storage calendar_remote]
type = "caldav"
url = "https://amaterasu.xyz/remote.php/dav"
auth = basic

